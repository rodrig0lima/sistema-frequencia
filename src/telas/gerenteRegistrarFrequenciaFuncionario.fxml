<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.GerenteRegistrarFrequenciaFuncionarioController">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="800.0">
         <menus>
            <Menu mnemonicParsing="false" text="Arquivo">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#gerentePrincipal" text="Início" />
                  <MenuItem mnemonicParsing="false" onAction="#sair" text="Sair" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Funcionários">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#gerenteCadastrarFuncionario" text="Cadastrar Funcionário" />
                  <MenuItem mnemonicParsing="false" onAction="#gerenteListarFuncionarios" text="Listar Funcionarios" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Ajuda">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#sobre" text="Sobre" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Label layoutX="313.0" layoutY="50.0" text="Frequência">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="saida" layoutX="357.0" layoutY="523.0" mnemonicParsing="false" onAction="#registrarSaida" text="Registrar Saida" visible="false">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="entrada" layoutX="350.0" layoutY="523.0" mnemonicParsing="false" onAction="#registrarEntrada" text="Registrar Entrada">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextArea fx:id="observacao" layoutX="195.0" layoutY="433.0" prefHeight="69.0" prefWidth="522.0" />
      <Label layoutX="106.0" layoutY="433.0" text="Observação:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="hora" layoutX="483.0" layoutY="391.0" promptText="HH:MM:SS" />
      <Label layoutX="426.0" layoutY="393.0" text="Hora:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="data" editable="false" layoutX="163.0" layoutY="391.0" promptText="DD/MM/AAAA" style="-fx-background-color: #eeeeee;" />
      <Label layoutX="106.0" layoutY="393.0" text="Data:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="nome" layoutX="173.0" layoutY="353.0" prefHeight="20.0" prefWidth="566.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="106.0" layoutY="343.0" text="Nome:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TableView fx:id="tabelaFrequencia" layoutX="98.0" layoutY="156.0" prefHeight="174.0" prefWidth="649.0">
         <columns>
            <TableColumn fx:id="colunaData" prefWidth="205.0" text="Data" />
            <TableColumn fx:id="colunaHoraEntrada" prefWidth="233.0" text="Hora de Entrada" />
            <TableColumn fx:id="colunaHoraSaida" prefWidth="207.0" text="Hora de Saída" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
